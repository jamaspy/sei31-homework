<h1>All Directors</h1>
<% @directors.each do |director| %>
<%= image_tag director.image %>
<ul>
  <li>Name: <%= link_to director.name, director_path(director.id)  %></li>
  <li>Nationality: <%= director.nationality %></li>
  <li>DOB: <%= director.dob.strftime '%A, %B %e, %Y' %></li>
  <li>Style <%= director.style %></li>
</ul>
<% end %>
